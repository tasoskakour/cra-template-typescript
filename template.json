			"@mui/icons-material": "^5.0.1",
			"@mui/material": "^5.0.1",
			"@sentry/browser": "^6.7.0",
			"date-fns": "^2.25.0",
			"graphql": "^15.6.1",
			"history": "^5.0.1",
			"notistack": "^2.0.2",
			"ramda": "^0.27.1",
			"react": "^17.0.2",
			"react-dom": "^17.0.2",
			"react-error-boundary": "^3.1.3",
			"react-ga": "^3.3.0",
			"react-router": "^6.0.0-beta.5",
			"react-router-dom": "^6.0.0-beta.5",
			"react-scripts": "4.0.3",
			"web-vitals": "^2.1.0",
			"workbox-background-sync": "^6.3.0",
			"workbox-broadcast-update": "^6.3.0",
			"workbox-cacheable-response": "^6.3.0",
			"workbox-core": "^6.3.0",
			"workbox-expiration": "^6.3.0",
			"workbox-google-analytics": "^6.3.0",
			"workbox-navigation-preload": "^6.3.0",
			"workbox-precaching": "^6.3.0",
			"workbox-range-requests": "^6.3.0",
			"workbox-routing": "^6.3.0",
			"workbox-strategies": "^6.3.0",
			"workbox-streams": "^6.3.0",
			"zustand": "^3.5.12"
		},
		"devDependencies": {
			"@cypress/code-coverage": "^3.9.11",
			"@cypress/instrument-cra": "^1.4.0",
			"@types/node": "^16.10.3",
			"@types/ramda": "^0.27.45",
			"@types/react": "^17.0.27",
			"@types/react-dom": "^17.0.9",
			"@typescript-eslint/eslint-plugin": "^4.33.0",
			"@typescript-eslint/parser": "^4.33.0",
			"babel-plugin-import": "^1.13.3",
			"cypress": "^8.5.0",
			"eslint": "^7.32.0",
			"eslint-config-prettier": "^8.3.0",
			"eslint-config-tasoskakour-typescript-prettier": "^1.2.14",
			"eslint-import-resolver-typescript": "^2.5.0",
			"eslint-plugin-cypress": "^2.12.1",
			"eslint-plugin-import": "^2.24.2",
			"eslint-plugin-jsx-a11y": "^6.4.1",
			"eslint-plugin-prettier": "^4.0.0",
